<aura:component>
    <aura:attribute name="numbers" type="List"/>
    <ui:button label="Display Numbers" press="{!c.getNumbers}"/>

    <!-- No need to reinvent the wheel! -->
    <aura:iteration var="number" items="{!v.numbers}">
        <br></br>
        {!number.value} 
    </aura:iteration>
</aura:component>