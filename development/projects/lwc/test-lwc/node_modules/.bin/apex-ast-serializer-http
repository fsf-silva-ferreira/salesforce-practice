#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/sh" ]; then
  "$basedir/sh"  "$basedir/../prettier-plugin-apex/vendor/apex-ast-serializer/bin/apex-ast-serializer-http" "$@"
  ret=$?
else 
  sh  "$basedir/../prettier-plugin-apex/vendor/apex-ast-serializer/bin/apex-ast-serializer-http" "$@"
  ret=$?
fi
exit $ret
